@page "/"
@using AppRegShared
@using Microsoft.AspNetCore.Authorization
@inject IAuthorizationService AuthorizationService

<MudContainer MaxWidth="MaxWidth.ExtraLarge" Class="mt-16">

	<AuthorizeView>
		<Authorized>
			<UserRegList></UserRegList>
		</Authorized>
		<NotAuthorized>
			<MudText Typo="Typo.h4" Align="Align.Center">Please log in to continue</MudText>
		</NotAuthorized>
	</AuthorizeView>

</MudContainer>


